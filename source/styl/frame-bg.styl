//replace with css effects

.frame_bg,
.i-frame-bg
   display inline-block
   text-shadow 0 1px 0 #ffffff
   font-size 8pt
   line-height normal
   color #666666
   
.frame_bg span.left, .i-frame-bg_left
   display inline-block
   padding-left 5px
   background url(/images/buttons.png) no-repeat 0 -852px
   
.frame_bg span.bg, .i-frame-bg_bg
   display inline-block
   background url(/images/buttons.png) repeat-x 0 -936px
   height 27px
   vertical-align  top

.frame_bg span.bg span, .i-frame-bg_content
   display inline-block
   padding 6px 5px 0 5px
   
.frame_bg span.right, .i-frame-bg_right
   display inline-block
   padding-right 5px
   background url(/images/buttons.png) no-repeat right -1020px

.frame_bg:hover,
.i-frame-bg:hover
  color #666666
  
.frame_bg:hover span.left,
.i-frame-bg:hover .i-frame-bg_left
  background-position 0 -880px
  
.frame_bg:hover span.bg,
.i-frame-bg:hover .i-frame-bg_bg
  background-position 0 -964px
  
.frame_bg:hover span.right,
.i-frame-bg:hover .i-frame-bg_right
  background-position right -1048px
  
.frame_bg:active,
.i-frame-bg:active
  color #666666
  
.frame_bg:active span.left,
.i-frame-bg:active .i-frame-bg_left
  background-position 0 -908px
  
.frame_bg:active span.bg,
.i-frame-bg:active .i-frame-bg_bg
  background-position 0 -992px
  
.frame_bg:active span.right
.i-frame-bg:active .i-frame-bg_right
  background-position right -1076px