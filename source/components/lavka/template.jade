.b-lavka
      
  .b-lavka__blocks
  
    .b-collection-block.b-store-block
      .b-collection-block__heading
        span.b-collection-block__heading__content Железо и огонь
      
      .row
        - var i = 0
        while i < 3
          - i++
          
          if i!=1
            .b-store-block__hr.visible-xs
          
          .col-sm-4
            .b-store-block__item__logo
              a(href='', target='_blank', rel='nofollow')
                img(src='/upload/store-block/logo.gif', height='30', alt='')
                
            .b-store-block__item__photo
              a.b-store-block__item__photo__link(href='', target='_blank', rel='nofollow')
                img(src='/upload/store-block/photo.jpg', width='90', height='130', alt='Гриль Bar-B-Kettle')
                
            .b-store-block__item__title
              a(href='', target='_blank', rel='nofollow') Гриль Bar-B-Kettle
              
            .b-store-block__item__price.b-price
              span.b-price__num
                span.b-price__num__int 5 346
                span.b-price__num__fract .60
              span.b-price__unit руб.
              
            .b-store-block__item__button
              a.btn.btn-default(href='', target='_blank', rel='nofollow') Купить
              
