                img(src='http://zhelezoiogon.ru/components/com_virtuemart/show_image_in_imgtag.php?filename=resized%2F_________________4eebaf5590b16_185x214.jpg&newxsize=185&newysize=214&fileout=', width='180', alt='Голова льва, ёмкости для соуса, 2 шт., 7 мл, фарфор')
            .b-store-block__item__title
              a(href='', target='_blank', rel='nofollow') "Голова льва", ёмкости для соуса, 2 шт., 7 мл, фарфор
            .b-store-block__item__price.b-price
