@import '../../styl/component-include.styl'

.b-heading-linethrough
  border-top 1px solid #ededed
  position relative
  margin 20px 0
  
  h2
    position absolute
    top -11px
    left 0
    width 100%
    text-align center
    margin 0
    
    span,
    a
      display inline-block
      background-color #fff
      padding 0 3px